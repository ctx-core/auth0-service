{
	"name": "@ctx-core/auth0-service",
	"version": "4.0.13",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-service#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-service/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-service.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.d.ts",
	"svelte": "./src/index.js",
	"exports": {
		".": {
			"browser": "./src/index.browser.js",
			"node": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/auth0": "^32.0.10",
		"@ctx-core/auth0-management": "^9.0.18",
		"@ctx-core/error": "^11.3.1",
		"@ctx-core/fetch-undici": "^1.1.5",
		"@ctx-core/jwt": "^10.0.51",
		"@ctx-core/logger": "^9.0.44",
		"@ctx-core/object": "*",
		"auth0-js": "^9.19.0",
		"jsonwebtoken": "^8.5.1"
	},
	"devDependencies": {
		"@types/jsonwebtoken": "^8.5.8",
		"@types/trouter": "^3.1.1",
		"c8": "^7.11.0",
		"check-dts": "^0.6.6",
		"svelte": "^3.46.4",
		"tsm": "^2.2.1",
		"typescript": "next",
		"uvu": "^0.5.3"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}
