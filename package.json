		"jsonwebtoken": "^8.5.1",
		"undici": "^5.11.0"
	},
	"devDependencies": {
		"@types/auth0-js": "^9.14.6",
		"@types/jsonwebtoken": "^8.5.9",
		"@types/trouter": "^3.1.1",
		"c8": "^7.12.0",
		"check-dts": "^0.6.7",
		"svelte": "^3.52.0",
		"tsx": "^3.10.3",
		"typescript": "next",
