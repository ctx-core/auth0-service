{
	"name": "@ctx-core/auth0-service",
	"version": "2.0.1",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"browser": "./dist/index.browser.js",
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^27.0.1",
		"@ctx-core/auth0-management": "workspace:^7.0.1",
		"@ctx-core/error": "workspace:^11.1.9",
		"@ctx-core/fetch": "workspace:^11.0.93",
		"@ctx-core/jwt": "workspace:^10.0.38",
		"@ctx-core/logger": "workspace:^9.0.32",
		"@ctx-core/object": "workspace:^22.0.0",
		"auth0-js": "^9.18.0",
		"jsonwebtoken": "^8.5.1",
		"polka": "^0.5.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/jsonwebtoken": "^8.5.6",
		"@types/polka": "^0.5.3",
		"@types/trouter": "^3.1.0",
		"svelte": "^3.44.3",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
