{
	"name": "@ctx-core/auth0-service",
	"version": "4.5.15",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-service#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-service/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-service.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./auth0__access_token_o__new",
		"./auth0__change_password__POST",
		"./auth0__header__authorization__access_token_o__validate",
		"./auth0__header__authorization__email__validate",
		"./auth0__header__authorization__user_id__validate",
		"./auth0__jwks_cert__new",
		"./auth0__jwks_x5c__new"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/auth0": "^40.1.12",
		"@ctx-core/auth0-management": "^9.8.13",
		"@ctx-core/error": "^12.4.7",
		"@ctx-core/fetch-undici": "^3.1.12",
		"@ctx-core/function": "^21.22.3",
		"@ctx-core/jwt": "^10.5.8",
		"@ctx-core/logger": "^9.2.3",
		"@ctx-core/object": "^30.4.0",
		"jsonwebtoken": "^9.0.2"
	},
	"devDependencies": {
		"@types/auth0": "^3.3.9",
		"@types/auth0-js": "^9.21.4",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/trouter": "^3.1.4",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}