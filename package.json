{
	"name": "@ctx-core/auth0-service",
	"version": "2.0.5",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-service#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-service/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-service.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"browser": "./dist/index.browser.js",
			"node": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "tsc src/index.ts --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^27.0.5",
		"@ctx-core/auth0-management": "workspace:^7.0.5",
		"@ctx-core/error": "workspace:^11.1.13",
		"@ctx-core/fetch": "workspace:^11.0.97",
		"@ctx-core/jwt": "workspace:^10.0.42",
		"@ctx-core/logger": "workspace:^9.0.36",
		"@ctx-core/object": "workspace:^22.0.4",
		"auth0-js": "^9.18.0",
		"jsonwebtoken": "^8.5.1",
		"polka": "^0.5.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/jsonwebtoken": "^8.5.6",
		"@types/polka": "^0.5.3",
		"@types/trouter": "^3.1.0",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.3",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
